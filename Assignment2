package javahome;

public class Assignment_2 {
	 int no1,no2,no3;
		public static void main(String[] args)
		{
			Assignment_2 t=new Assignment_2();
			t.no1=30;
			t.no2=40;
			t.no3=20;
			t.minNumber();
		}
		void minNumber()
		{
			if(no1<no2 && no1<no3)
			{
				System.out.println("Number One Is Less Than The Other Two Numbers");
			}
			else if(no2<no1 && no2<no3)
			{
				System.out.println("Number Two Is Less Than The Other Two Numbers ");
			}
			else if(no3<no1 && no3<no2)
			{
				System.out.println("Number Three Is Less Than The Other Two Numbers");
			}
			else
			{
				System.out.println("All The Numbers Are Equal");
			}
		}

	 

}
